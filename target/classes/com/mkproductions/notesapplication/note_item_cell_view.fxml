<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root fx:id="mNoteListCell" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
         prefWidth="NaN"
         style="-fx-background-radius: 10; -fx-border-radius: 10;" type="javafx.scene.control.ListCell"
         xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <graphic>
        <AnchorPane fx:id="mNoteAnchorPane" prefHeight="50.0" prefWidth="NaN"
                    style="-fx-border-radius: 10; -fx-background-radius: 10;">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label fx:id="mNoteTopic" alignment="CENTER" contentDisplay="CENTER" text="Label"
                   AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="4.0">
                <font>
                    <Font size="40.0"/>
                </font>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </Label>
            <Button fx:id="mNoteDeleteButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    text="Delete" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                <font>
                    <Font size="20.0"/>
                </font>
            </Button>
        </AnchorPane>
    </graphic>
</fx:root>
